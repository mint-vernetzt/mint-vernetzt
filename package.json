{
  "name": "mint-vernetzt",
  "repository": {
    "type": "git",
    "url": "https://github.com/mint-vernetzt/mint-vernetzt"
  },
  "scripts": {
    "prepare": "husky install",
    "create-react-app": "nx g @nrwl/react:application --e2eTestRunner=none --globalCss --pascalCaseFiles --routing=false --style=css",
    "create-react-lib": "nx g @nrwl/react:library --style=none --buildable --publishable --pascalCaseFiles --component=false",
    "create-react-lib-component": "nx g @nrwl/react:component --style=none --export --pascalCaseFiles --flat",
    "serve-design-system": "nx tailwind design-system | nx storybook design-system",
    "build-design-system": "nx build-tailwind design-system && nx build-storybook design-system"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "document-register-element": "1.13.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "regenerator-runtime": "0.13.7",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.12.13",
    "@babel/preset-env": "7.12.13",
    "@babel/preset-react": "7.12.13",
    "@babel/preset-typescript": "7.12.13",
    "@nrwl/cli": "12.6.2",
    "@nrwl/jest": "12.6.2",
    "@nrwl/react": "^12.6.2",
    "@nrwl/storybook": "12.6.2",
    "@nrwl/tao": "12.6.2",
    "@nrwl/web": "12.6.2",
    "@nrwl/workspace": "12.6.2",
    "@storybook/addon-a11y": "^6.3.6",
    "@storybook/addon-essentials": "~6.3.0",
    "@storybook/react": "~6.3.0",
    "@svgr/webpack": "^5.4.0",
    "@testing-library/react": "11.2.6",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "26.0.24",
    "@types/node": "14.14.33",
    "@types/react": "17.0.3",
    "@types/react-dom": "17.0.3",
    "autoprefixer": "^10.3.1",
    "babel-jest": "27.0.6",
    "babel-loader": "8.1.0",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "8.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.23.1",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "^7.0.0",
    "jest": "27.0.3",
    "lerna": "^4.0.0",
    "lint-staged": "^11.1.2",
    "postcss": "^8.3.6",
    "prettier": "^2.3.1",
    "storybook-dark-mode": "^1.0.8",
    "tailwindcss": "^2.2.7",
    "ts-jest": "27.0.3",
    "ts-node": "~9.1.1",
    "typescript": "~4.3.5",
    "url-loader": "^3.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{js,css,md,ts,tsx,json}": "prettier --write"
  }
}
